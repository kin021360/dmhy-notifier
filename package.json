  "license": "MIT",
  "dependencies": {
    "@elastic/ecs-pino-format": "1.3.0",
    "level-ttl": "3.1.1",
    "leveldown": "6.1.0",
    "levelup": "5.1.1",
    "module-alias": "2.2.2",
    "moment-timezone": "0.5.34",
    "node-telegram-bot-api": "^0.56.0",
    "pino": "^7.0.0",
    "pino-elasticsearch": "6.2.0",
    "pino-multi-stream": "^6.0.0",
    "pino-pretty": "7.3.0",
    "rss-parser": "3.12.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/level-ttl": "^3.1.1",
    "@types/leveldown": "^4.0.2",
    "@types/levelup": "^4.3.0",
    "@types/node": "^14.14.35",
