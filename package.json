{
  "name": "dmhy-notifier",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kin021360/dmhy-notifier"
  },
  "author": "Nathan Lam",
  "license": "MIT",
  "dependencies": {
    "@elastic/ecs-pino-format": "1.3.0",
    "level-ttl": "3.1.1",
    "leveldown": "6.1.1",
    "levelup": "5.1.1",
    "module-alias": "2.2.2",
    "moment-timezone": "0.5.40",
    "node-telegram-bot-api": "0.58.0",
    "pino": "8.8.0",
    "pino-elasticsearch": "6.3.0",
    "pino-pretty": "9.1.1",
    "rss-parser": "3.13.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@types/level-ttl": "3.1.2",
    "@types/leveldown": "4.0.5",
    "@types/levelup": "5.1.2",
    "@types/node": "16.18.23",
    "@types/node-telegram-bot-api": "0.57.6",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "eslint": "8.31.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "prettier": "2.8.2",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.5.4"
  },
  "scripts": {
    "ichk": "yarn install --check-files",
    "start": "node --expose-gc ./dist/index.js",
    "dev": "node -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "prebuild": "shx rm -rf dist",
    "build": "tsc",
    "format": "prettier --write src/** && yarn lint --fix",
    "lint": "eslint ./src/ --ext .js,.jsx,.ts,.tsx"
  },
  "_moduleAliases": {
    "src": "./dist"
  }
}
